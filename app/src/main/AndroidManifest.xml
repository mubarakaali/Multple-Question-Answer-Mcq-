<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fivesol.kapsulemcqs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhotoEditor.NoActionBar">
        <activity
            android:name=".presentation.old.SubCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.ui.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.main_category.MainActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".presentation.old.SubCategoryActivitySlider"
            android:screenOrientation="portrait"/>
    </application>

</manifest>